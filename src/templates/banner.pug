section.banner(data-animate='clip-path')
  .banner-bg
    .banner-bg__back
      .banner-bg__back__container
        img(src=require('@/assets/img/banner-bg-back.jpg'), alt='banner-back')

    .banner-bg__img
      .banner-bg__img__container
        img(
          src=require('@/assets/img/banner-bg-front.png'),
          alt='banner-front'
        )

  ._container.banner__container
    .banner__wrapper
      .banner__header
        p.banner__phrase.medium.desktop.left купимо <br/>та доставимо <br/>“під ключ”

        p.banner__phrase.small.right Американська <br/>компанія з українським <br/>характером

      .banner__main
        p.banner__phrase.medium.mobile купимо <br/>та доставимо <br/>“під ключ”

        h1.banner__title!= 'вигідне <br/>авто з США '

        .banner-steps
          .banner-steps__container
            .banner-steps__item.left
              p <span>Вкажіть</span> бажаний автомобіль

            .banner-steps__item.right
              p <span>Все інше</span> зробить W8shipping

        .banner__buttons
          a.banner__button.button-empty(href='#') 
            span Обрати бажане авто

          a.banner__button.button-filled(href='#') 
            span отримати підбір авто

      .banner__footer
        ul.socials.banner__socials
          each item in socialsBanner
            li
              a.socials__link(
                href=`${item.href}`,
                target='_blank',
                rel='noreferrer'
              )!= item.label

        button.banner__down
          <svg width="54" height="54" viewBox="0 0 54 54" fill="none" xmlns="http://www.w3.org/2000/svg">
            <circle cx="27" cy="27" r="26" stroke="currentColor" stroke-width="2"/>
            <path d="M27 14L27 39.5M37 30L27 40L17 30" stroke="currentColor" stroke-width="2"/>
          </svg>
